<% function user(user) { %>
		<tr>
		<!-- Loop through locations, find by location_num -->
		  <td> <a href="<%= user.website %>"><%= user.display_name%></a></td>
		  <td><%= user.rating.yelp_rating %>(<%= user.rating.yelp_reviews %>)</td>	
		  <td>$<%= user.avg_price %></td>
		  <!-- Code to loop through locations and select each location SEMI-->
		  <td><%= user.avg_price %></td>
		  <% for(var i=0; i<user.locations; i++) {%>
		  	<!-- Console.logging output right now, change to index array contents -->
		  	<!--<td><%= user.locations[i].phone %></td>-->
		  	<td><%= user.avg_price %></td>
		  <% } %>
		</tr>
<% } %>

<html>
<head>
<style>
	table,th,td
	{border:0.5px solid black; text-align:center; vertical-align: middle}
</style>
		</head>
		<body>
		<h1>Making dynamic HMTL table from DB data to port over to site</h1>
		<table style="width:300px">
	<!-- TABLE HEADER -->
		<tr>
			<th style="width: 20%"> Name</th>
			<th style="width: 20%;"> Rating</th>
			<th style="width: 20%;"> Avg. Price</th>
			<th style="width: 10%"> Location</th>
		</tr>
	<!-- /TABLE HEADER -->
	<% users.map(user) %>	
	</table>
</body>
</html>