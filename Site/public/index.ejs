<!-- EJS for dynamic table on home page -->
<% function user(user) { %>
  <% for(var i=0; i<user.locations.length; i++) { if(i==0){ %>
    <tr>
      <td> <a href="<%= user.website %>"> <%= user.display_name%></a>,   <%= user.locations[i].location_name %></td>
        <td> <%= user.locations[i].address %> </td>
        <td><%= user.rating.yelp_rating %>(<%= user.rating.yelp_reviews %>)</td>
        <td>%<%= user.avg_price %></td>
    </tr>
  <% } }%>
<% } %>

